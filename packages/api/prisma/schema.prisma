generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:postgres@localhost:5432/spacedrop"
}

model Account {
  id       Int    @id @default(autoincrement())
  username String @unique
  email    String @unique
  plan     Plan

  @@map("accounts")
}

enum Plan {
  FREE
  TEIR1
  TEIR2
}

model Client {
  id         String  @id
  ip_address String?

  @@map("clients")
}
