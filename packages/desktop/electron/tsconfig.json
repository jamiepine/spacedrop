{
  "compilerOptions": {
    "target": "ES5",
    "outDir": "../build/electron",
    "skipLibCheck": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "strict": true,
    "incremental": true,
    "esModuleInterop": true,
    "rootDir": "."
  }
}
